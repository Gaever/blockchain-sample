{
  "name": "@ctocker/ctocks",
  "version": "0.0.1",
  "license": "ISC",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "start": "node dist/src/main.js",
    "build": "rm -rf ./dist && tsc --project ./tsconfig.json && tscpaths --project ./tsconfig.json -s ./ -o ./dist",
    "test:unit": "ts-mocha -p tsconfig.json --paths --recursive --timeout 10000 --exit ./src/tests/index-unit.test.ts",
    "test:func": "ts-mocha -p tsconfig.json --paths --recursive --timeout 60000 --exit ./src/tests/index-func.test.ts",
    "test:integ": "ts-mocha -p tsconfig.json --paths --recursive --timeout 600000 --bail --exit ./src/tests/index-integ.test.ts",
    "seed": "knex seed:run",
    "migrate": "knex migrate:latest --verbose",
    "unmigrate": "knex migrate:rollback --verbose"
  },
  "dependencies": {
    "@ctocker/lib": "^0.0.1",
    "@types/node-fetch": "^3.0.3",
    "@types/validate.js": "^0.11.0",
    "axios": "^0.21.4",
    "dotenv": "^10.0.0",
    "argparse": "^2.0.1",
    "knex": "^0.95.13",
    "module-alias": "^2.2.2",
    "node-fetch": "^3.0.0",
    "pubsub-js": "^1.9.3",
    "socket.io": "^4.3.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/dotenv": "^8.2.0",
    "@types/pubsub-js": "^1.8.3",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "moment": "^2.29.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-mocha": "^8.0.0",
    "tsconfig-paths": "^3.10.1"
  }
}
